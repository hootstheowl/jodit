{
	"name": "jodit",
	"version": "4.0.0-beta.61",
	"description": "Jodit is awesome and usefully wysiwyg editor with filebrowser",
	"main": "build/jodit.min.js",
	"types": "./types/index.d.ts",
	"config": {
		"excludePlugins": "",
		"includePlugins": "wrap-nodes,hotkeys,source,format-block,link,bold,image,resizer,color,clean-html,iframe,backspace,redo-undo,size,hr,indent,table,select-cells,table-keyboard-navigation,ordered-list,justify,",
		"excludeLanguages": "",
		"includeLanguages": "en,",
		"ecma": "es2018"
	},
	"scripts": {
		"start": "make start es=$npm_package_config_ecma uglify=true includePlugins=\"$npm_package_config_includePlugins\" excludeLanguages=\"$npm_package_config_excludeLanguages\" includeLanguages=\"$npm_package_config_includeLanguages\"",
		"clean": "make clean",
		"lint": "make lint",
		"coverage": "make coverage",
		"newversion": "make newversion",
		"build-single": "make build-single es=$npm_package_config_ecma excludePlugins=\"$npm_package_config_excludePlugins\" includePlugins=\"$npm_package_config_includePlugins\" excludeLanguages=\"$npm_package_config_excludeLanguages\" includeLanguages=\"$npm_package_config_includeLanguages\"",
		"build": "make build-all excludePlugins=\"$npm_package_config_excludePlugins\" includePlugins=\"$npm_package_config_includePlugins\" excludeLanguages=\"$npm_package_config_excludeLanguages\" includeLanguages=\"$npm_package_config_includeLanguages\"",
		"test": "make test",
		"test:screenshots": "make screenshots-test",
		"test:debug": "make test-only-run browsers=Chrome singleRun=false",
		"test:chrome": "make test browsers=Chrome",
		"jodit": "make jodit",
		"pretty": "make prettify",
		"fix": "make fix",
		"type:build": "make dts"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/xdan/jodit.git"
	},
	"keywords": [
		"wysiwyg",
		"wysiwyg editor",
		"wysiwyg html editor",
		"rich text",
		"rich editor",
		"rich text editor",
		"html",
		"text",
		"editor",
		"contenteditable",
		"javascript",
		"typescript",
		"vanillajs",
		"jodit"
	],
	"author": "Chupurnov <chupurnov@gmail.com> (https://xdsoft.net/jodit/)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/xdan/jodit/issues"
	},
	"homepage": "https://xdsoft.net/jodit/",
	"dependencies": {
		"autobind-decorator": "^2.4.0"
	},
	"devDependencies": {
		"@types/ace": "^0.0.48",
		"@types/fs-extra": "^11.0.1",
		"@types/karma": "^6.3.3",
		"@types/node": "^20.1.0",
		"@types/postcss-css-variables": "^0.18.0",
		"@types/yargs": "^17.0.24",
		"@typescript-eslint/eslint-plugin": "^5.59.2",
		"@typescript-eslint/parser": "^5.59.2",
		"autoprefixer": "^10.4.14",
		"axios": "^1.4.0",
		"chai": "^4.3.7",
		"classlist-polyfill": "^1.2.0",
		"compression": "^1.7.4",
		"core-js": "^3.30.2",
		"cross-env": "^7.0.3",
		"css-loader": "^6.7.3",
		"css-minimizer-webpack-plugin": "^5.0.0",
		"cssnano-preset-advanced": "^6.0.1",
		"es6-promise": "^4.2.8",
		"eslint": "^8.40.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-header": "^3.1.1",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-tsdoc": "^0.2.17",
		"glob": "^10.2.2",
		"karma": "^6.4.2",
		"karma-chai": "^0.1.0",
		"karma-chrome-launcher": "^3.2.0",
		"karma-firefox-launcher": "^2.1.2",
		"karma-mocha": "^2.0.1",
		"karma-sourcemap-loader": "^0.4.0",
		"less": "^4.1.3",
		"less-loader": "^11.1.0",
		"lint-staged": "^13.2.2",
		"merge-stream": "^2.0.0",
		"mini-css-extract-plugin": "^2.7.5",
		"mocha": "^10.2.0",
		"node-jq": "^2.3.5",
		"open": "^9.1.0",
